source ~/.bashrc
shopt -s expand_aliases
oci() {
  docker run --rm  \
    -e "OCI_CLI_SUPPRESS_FILE_PERMISSIONS_WARNING=True" \
    -v "$HOME/.oci:/oracle/.oci" oci "$@"
}
oci network security-list update \
  --security-list-id $SL \
  --ingress-security-rules file://.oci/ingress.json  \
  --egress-security-rules file://.oci/egress.json
  
